<ul class="list-none">
    <% for(var i=0; i<data.length; i++) {%>
      <li class="election-Item">
        <div class="flex items-center w-full border border-black-900  w-full p-1 my-2 px-1 py-3 rounded hover:bg-purple-50">
            
              <label class="ml-1  text-2xl w-full text-black-600 cursor-pointer">
                <%= data[i].title %>
              </label>
              <button class="bg-green-600 px-5 py-2 hover:bg-green-800 text-xl rounded px-5 py-2 text-white" onclick="window.location.href='/elections1/<%= data[i].id%>';">View
              </button>
        </div>
      </li>
      <% } %> 
  </ul>