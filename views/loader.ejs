<ul class="list-none">
    <% for(var i=0; i<data.length; i++) {%>
      <li class="election-Item">
        <div class="flex items-center w-full border border-black-900  w-full p-1 my-2 px-1 py-3 rounded hover:bg-purple-50">
            
              <label class="ml-1  text-2xl w-full text-black-600 cursor-pointer">
                <%= data[i].title %>
              </label>

             

              <div class="py-2 px-5">
                <button  name="complete" class="text-white bg-green-600 hover:bg-green-800 font-medium rounded px-5 py-2"><a href="" onclick="updateelection(<%= data[i].id %>)">END</a></button>
              </div>
              
               
              <button class="text-white bg-green-600 hover:bg-green-800 font-medium rounded px-5 py-2"><a href="/result/<%= data[i].id%>" name="view" >view</a></button>
        </div>
      </li>
      <% } %>
  </ul>