<ul class="list-none">
    <% for(var i=0; i<data.length; i++) {%>
      <li class="election-Item">
        <div class="flex items-center w-full border border-black-900  w-full p-1 my-2 px-1 py-3 rounded hover:bg-purple-50">
            
              <label class="ml-1  text-2xl w-full text-black-600 cursor-pointer">
                <%= data[i].title %>
              </label>

             

              <div class="py-2 px-5">
                <a href="" class="px-5 py-3 border border-red-300 bg-green-600 hover:bg-green-800 text-white rounded" onclick="updateelection(<%= data[i].id %>)">END</a>
              </div>
              
              <button class="bg-green-600 px-5 py-2 hover:bg-green-800 text-xl rounded px-5 py-2 text-white" onclick="window.location.href='/result/<%= data[i].id%>';">View
              </button>
        </div>
      </li>
      <% } %>
  </ul>